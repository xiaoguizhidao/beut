<?xml version="1.0" encoding="UTF-8"?>
<config>
    <tabs>
        <mage_custom translate="label" module="kjbsetup">
            <label>Mage Exten</label>
            <sort_order>0</sort_order>
        </mage_custom>
    </tabs>  

    <sections>
        <kjbsetup translate="label" module="kjbsetup">
            <label>Web-form Question</label>
            <tab>mage_custom</tab>
            <frontend_type>text</frontend_type>
            <sort_order>11032</sort_order>
            <show_in_default>1</show_in_default>
            <show_in_website>1</show_in_website>
            <show_in_store>1</show_in_store>
            <groups>
                <general translate="label">
                    <label>General Settings</label>
                    <frontend_type>text</frontend_type>
                    <sort_order>5</sort_order>
                    <show_in_default>1</show_in_default>
                    <show_in_website>1</show_in_website>
                    <show_in_store>1</show_in_store>
                    <fields>
                        <admin_email translate="label comment">
                            <label>Admin Email</label>
                            <frontend_type>text</frontend_type>
                            <sort_order>20</sort_order>
                            <show_in_default>1</show_in_default>
                            <show_in_website>1</show_in_website>
                            <show_in_store>1</show_in_store>
                        </admin_email>
                        <template_question_email translate="label">
                            <label>Template Email</label>
                            <frontend_type>select</frontend_type>
                             <source_model>adminhtml/system_config_source_email_template</source_model>
                            <sort_order>100</sort_order>
                            <show_in_default>1</show_in_default>
                            <show_in_website>1</show_in_website>
                        </template_question_email>

                        <subject_email translate="label comment">
                            <label>Subject Email</label>
                            <frontend_type>text</frontend_type>
                            <sort_order>150</sort_order>
                            <show_in_default>1</show_in_default>
                            <show_in_website>1</show_in_website>
                            <show_in_store>1</show_in_store>
                        </subject_email>
                    </fields>
                </general>
            </groups>
        </kjbsetup>
    </sections>
</config>